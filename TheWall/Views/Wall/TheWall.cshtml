@{
    int? LoggedInUser = Context.Session.GetInt32("UserId");
    string UserName = Context.Session.GetString("UserName");
}
@model List<Message>
<div class="mt-5">
    @await Html.PartialAsync("_MessageForm", new Message())

    @foreach (Message m in Model)
    {
        @await Html.PartialAsync("_MessageCard", m)   
    }
</div>